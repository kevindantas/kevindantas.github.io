{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "linebreak-style": "off",
    "react/jsx-filename-extension": "off",
    "no-unused-expressions": ["error", {
      "allowTaggedTemplates": true
    }]
  }
}